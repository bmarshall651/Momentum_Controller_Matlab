TECHFORMAT=V2

UNITS
  DISTANCE=METRE
  CONDUCTIVITY=SIEMENS/M
  RESISTIVITY=OHM.CM
  RESISTANCE=OHM/SQ
  PERMITTIVITY=RELATIVETOVACUUM
  PERMEABILITY=RELATIVETOVACUUM
  FREQUENCY=HZ
END_UNITS

BEGIN_MATERIAL
  MATERIAL Copper CONDUCTIVITY=5.8e+07 IMAG_CONDUCTIVITY=0
  MATERIAL FR_4 PERMITTIVITY=3.66 LOSSTANGENT=0.0127 PERMEABILITY=1 DJORDJEVIC LOWFREQ=1000 VALUEFREQ=5e+09 HIGHFREQ=1e+12
END_MATERIAL

BEGIN_OPERATION
  OPERATION OperationSHEET SHEET
  OPERATION OperationThickness_cond EXPAND=3.4798e-05 UP
  OPERATION OperationThickness_pc2 INTRUDE=0 UP
  OPERATION OperationDRILL DRILL
END_OPERATION

BEGIN_MASK
  MASK 1 Name=cond PRECEDENCE=0 COLOR="ee6a50" MATERIAL=Copper OPERATION=OperationThickness_cond
  MASK 17 Name=pc2 NEGATIVE PRECEDENCE=0 COLOR="2e8b57" MATERIAL=PERFECT_CONDUCTOR OPERATION=OperationThickness_pc2 MASK_PROPERTIES = { "MomModelType=operation2sheet" }
  MASK 25 Name=pcvia2 PRECEDENCE=0 COLOR="ffa07a" MATERIAL=Copper OPERATION=OperationDRILL
END_MASK

BEGIN_STACK
  TOP OPEN MATERIAL=AIR
  INTERFACE Name=__Interface3      MASK={cond}
  LAYER     Name=__SubstrateLayer2 MASK={pcvia2} HEIGHT=0.00017018 MATERIAL=FR_4
  INTERFACE Name=__Interface1      MASK={pc2}
  BOTTOM OPEN MATERIAL=AIR
END_STACK

